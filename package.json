{
  "name": "cypressautomationframework",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node_modules\\.bin\\cypress run",
    "chromeTest": "npm test -- --browser chrome",
    "edgeTest": "npm test -- --browser edge",
    "onlyreport": "npm test -- --browser chrome --reporter mocha-multi-reporters --reporter-options configFile=cypress\\configuration\\reporter-config.json",
    "recordDashboardTest": "npm test -- --record --key 903c3273-554f-4f77-a6d9-fe80586694c4",
    "report": "cypress run --record --key 903c3273-554f-4f77-a6d9-fe80586694c4 --reporter mocha-multi-reporters --reporter-options configFile=cypress\\configuration\\reporter-config.json",
    "delete:reportFolder": "del -f mochawesome-report/",
    "test:cli": "npm run delete:reportFolder && cypress run",
    "cy:run": "cypress run allure=true",
    "clean:folders": "rm -R -f allure-report/* && rm -R -f allure-results/*",
    "allure:report": "cypress run allure generate allure-results â€” clean -o allure-report && allure open allure-report",
    "beforetest": "npm run clean:folders",
    "tests": "npm run cy:run || npm run aftertest",
    "aftertest": "npm run allure:report"
  },
  "author": "SHIVAJI DHUMAL - P1334185",
  "license": "ISC",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.35.2",
    "allure-commandline": "^2.21.0",
    "cypress": "^12.7.0",
    "cypress-junit-reporter": "^1.3.1",
    "cypress-mochawesome-reporter": "^3.3.0",
    "mocha": "^10.2.0",
    "mocha-allure-reporter": "^1.4.0",
    "mocha-junit-reporter": "^2.2.0",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0",
    "start-server-and-test": "^2.0.0"
  }
}
